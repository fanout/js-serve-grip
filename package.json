{
  "name": "@fanoutio/serve-grip",
  "version": "1.3.0-beta.0",
  "author": "Fanout, Inc. <info@fanout.io>",
  "description": "Connect-style Middleware for GRIP",
  "packageManager": "pnpm@7.1.9",
  "contributors": [
    {
      "name": "Katsuyuki Ohmuro",
      "email": "kats@fanout.io"
    },
    {
      "name": "Konstantin Bokarius",
      "email": "kon@fanout.io"
    }
  ],
  "main": "./build/src/index.js",
  "module": "./build/esm/index.js",
  "files": [
    "browser/**/*",
    "build/**/*",
    "types/**/*"
  ],
  "types": "./build/src/index.d.ts",
  "scripts": {
    "prepack": "pnpm run build",
    "build-TEMPORARY_BACKUP": "pnpm test && pnpm lint && pnpm build-package && pnpm build-browser",
    "build": "pnpm build-package",
    "build-browser": "pnpm build-browser:clean && pnpm build-browser:build-dev && pnpm build-browser:build-prod",
    "build-browser:clean": "rimraf browser",
    "build-browser:build-dev": "NODE_ENV=development webpack",
    "build-browser:build-prod": "NODE_ENV=production webpack -p",
    "build-package": "pnpm build-package:clean && pnpm build-package:compile",
    "build-package:clean": "rimraf build",
    "build-package:compile": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "pnpm lint:eslint",
    "lint:eslint": "eslint src",
    "test": "ts-mocha -p tsconfig.json \"tests/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanout/js-serve-grip.git"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "grip",
    "fanout",
    "pushpin",
    "realtime",
    "push",
    "publish",
    "express",
    "connect",
    "nextjs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "mocha": "^10.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@fanoutio/grip": "^3.3.0",
    "callable-instance": "^2.0.0",
    "debug": "^4.1.1"
  }
}
